{
    "name": "S3 Ingest",
    "description": "Implements high assurance data upload via S3 protocol, followed by capture of file fixity data.",
    "components": [
        {
            "repo": "sd2e/process_upload:latest",
            "options": {
                "host": "chombo-staging.sd2e.org"
            }
        },
        {
            "repo": "sd2e/route_uploads:latest",
            "options": {
                "host": "chombo-staging.sd2e.org"
            }
        },
        {
            "uri": "amqp://wombat.rmq.cloudamqp.com/jrpzcdwt",
            "identifier": "taccmon-staging",
            "options": {"vhost": "jrpzcdwt"}
        },
        {
            "uri": "https://s3-data-upload.sd2e.org:9001/",
            "options": {
                "repo": "minio/mc:RELEASE.2017-10-14T00-51-16Z",
                "bucket": "uploads"
            }
        },
        {
            "uri": "agave://data-sd2e-community/uploads",
            "options": {
                "tenant": "sd2e",
                "owner": "sd2eadm"
                }
        },
        {
            "uri": "mongodb://catalog@catalog.sd2e.org:27020/admin?readPreference=primary",
            "options": {
                "database": "catalog",
                "username": "catalog",
                "replica": null
            }
        }
    ],
    "processing_levels": ["0"],
    "collections_levels": ["file"],
    "accepts": [
        "*"
    ],
    "produces": [
        "*"
    ],
    "pipeline_class": "data-transfer"
}
